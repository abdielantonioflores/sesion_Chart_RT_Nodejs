<div class="modal fade" id="modalOptions" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5> datos de {{name}}</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <form class="form-inline">
          <div class="form-group mb-2">
            <input type="text" class="form-control" id="level" placeholder="{{level}}">
          </div>
          <div class="form-group mx-sm-3 mb-2">
            <input type="text" class="form-control" id="value" placeholder="{{value}}">
          </div>
          <button type="submit" class="btn btn-dark mb-2" ng-click="updatedataChart()">Update data {{name}}</button>
        </form>
      </div>
    </div>
  </div>
</div>