            </div>
            <div class="col-3 text-center " id="NameChart">
            </div>
          </div>
          <div class="row">
            <div class="col-12">
              <canvas id="myChart3"></canvas>
            </div>
          </div>

        </div>
        <div class="col-6">
          <div class="row ">
            <div class="col-3">
              <select class="btn btn-dark btn-sm btn-block" name="myChart4" onchange=clickaction(this,this.name) id="myChart4_op">
              </select>
              <label id="myChart4_nameChart"></label>
            </div>
            <div class="col-3">
              <select class="btn btn-primary btn-sm btn-block" name="myChart4" onchange=clickaction(this,this.name) id="myChart4_name">
              </select>
            </div>
            <div class="col-3">
              <input name="myChart4" class="btn btn-danger btn-sm btn-block" type="button" value="Save" onclick="Save(this.name)">
            </div>
            <div class="col-3 text-center " id="NameChart">

            </div>
          </div>
          <div class="row">
            <div class="col-12">
              <canvas id="myChart4"></canvas>
            </div>
          </div>

        </div>
      </div>
    </div>

  </div>
  <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
